<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 7.4.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_7_4_0.dtd">
<routes>
	<route>
		<pattern>/{courseId:\d+}</pattern>
		<implicit-parameter name="mvcRenderCommandName">/courses/view_course</implicit-parameter>
		<implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
	</route>
	<route>
		<pattern>/{urlTitle}</pattern>
		<implicit-parameter name="mvcRenderCommandName">/courses/view_course</implicit-parameter>
		<implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
	</route>
	<route>
		<pattern>/lecture/{lectureId:\d+}</pattern>
		<implicit-parameter name="mvcRenderCommandName">/courses/view_lecture</implicit-parameter>
		<implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
	</route>
	<route>
		<pattern>/lecture/{urlTitle}</pattern>
		<implicit-parameter name="mvcRenderCommandName">/courses/view_lecture</implicit-parameter>
		<implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
	</route>
</routes>